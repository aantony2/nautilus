# Sample environment configuration for Nautilus Cloud Data Updater
# Copy this file to .env and modify with your actual values

# Database connection
DATABASE_URL=postgres://user:password@host:port/dbname

# Google Cloud credentials
GOOGLE_PROJECT_ID=my-gcp-project
# Note: GOOGLE_APPLICATION_CREDENTIALS is set to /app/secrets/google-credentials.json in the container

# Azure credentials
AZURE_TENANT_ID=xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx
AZURE_CLIENT_ID=xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx
AZURE_CLIENT_SECRET=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
AZURE_SUBSCRIPTION_ID=xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx

# AWS credentials
AWS_ACCESS_KEY_ID=AKIAXXXXXXXXXXXXXXXX
AWS_SECRET_ACCESS_KEY=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
AWS_REGION=us-west-2

# Log level (debug, info, warn, error)
LOG_LEVEL=info